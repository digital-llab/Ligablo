<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.llab.ligablo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".controllers.activities.Recouvrement"></activity>
        <activity android:name=".controllers.activities.Inventaire" />
        <activity android:name=".controllers.activities.Ventes" />
        <activity
            android:name=".controllers.activities.Formulaire_Extension"
            android:label="Extensions" />
        <activity
            android:name=".controllers.activities.Etablissement_Creation"
            android:label="Info de l&apos;Etablissement" />
        <activity
            android:name=".controllers.activities.DrawerDashBoard"
            android:label="Acceuil" />
        <activity
            android:name=".controllers.activities.EnregistrementActivity"
            android:label="Enregistrement"
            android:parentActivityName=".controllers.activities.MainActivity" />
        <activity
            android:name=".controllers.activities.ConnectionActivity"
            android:label="Connexion"
            android:parentActivityName=".controllers.activities.MainActivity" />
        <activity android:name=".controllers.activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>